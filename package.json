{
  "name": "aidenzich.github.io",
  "homepage": "https://aidenzich.github.io/",
  "version": "1.3.0",
  "dependencies": {
    "classnames": "2.2.6",
    "marked": "^3.0.7",
    "moment": "2.29.1",
    "node-sass": "^8.0.0",
    "nouislider": "14.6.4",
    "react": "17.0.2",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.2",
    "react-icons": "^4.7.1",
    "react-markdown": "^7.1.0",
    "react-medium-image-zoom": "^4.3.7",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-vertical-timeline-component": "^3.6.0",
    "reactstrap": "8.9.0",
    "remark-gfm": "^3.0.0",
    "typewriter-effect": "^2.18.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.css",
    "minify-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.css --source-map true",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "jquery": "3.6.0",
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "bootstrap": "4.6.0",
    "gh-pages": "^3.2.3",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "raw-loader": "^4.0.2"
  },
